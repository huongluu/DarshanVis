{
    "id": 8,
    "name": "Average and Maximum I/O Throughput of  chart",
    "type": "line",
    "template": "charts/line.js.php",
    "filter": "components/_filters_for_platform.php",
    "highchart-confs": {
        "title": "Median and Maximum I/O Throughput of All Applications",
        "chart": {
            "zoomType": "xy",
            "width": 800
        },
        "title": {
            "text": "Median and Maximum I/O Throughput of All Applications",
            "margin": -2,
            "style": {
                "margin-top": "40px"
            }
        },
        "subtitle": "",
        "xAxis": {
            "min": 1,
            "labels": {
                "format": "{value}",
                "style": {
                    "font-size": "12px"
                }
            },
            "title": {
                "text": "Applications",
                "style": {
                    "font-size": "13px"
                }
            },
            "crosshair": true
        },
        "yAxis": {
            "type": "logarithmic",
            "labels": {
                "style": {
                    "font-size": "14px"
                }
            },
            "title": {
                "text": "I/O Throughput  (logarithmic scale)",
                "style": {
                    "font-size": "15px"
                }
            }

        },
        "tooltip": {
            "valueSuffix": "MB/s",
            "shared": true

        },
        "plotOptions": {
            "series": {
                "allowPointSelect": true
            }
        }
    },
    "query": {
        "select": "appname,max(agg_perf_MB) as max_thruput, median(agg_perf_MB) as median_thruput",
        "from": "jobs_info",

        "where": "agg_perf_MB > 0 ",
        "group": "appname",
        "order": "max_thruput"
    },
    "sorting": [
        "max_thruput",
        "median_thruput"
    ],
    "series": [
        {
            "attribute": "median_thruput",
            "name": "Median I/O Throughput",
            "description": "",
            "color": "#CC0000",
            "type": "line"
        },
        {
            "attribute": "max_thruput",
            "name": "Maximum I/O Throughput",
            "description": "",
            "color": "#33FF99",
            "type": "line"
        }
    ]
}